--1
SELECT 
Nome,
Ano
FROM Filmes

--2

Select
Id,
Nome,
Ano,
Duracao FROM Filmes
ORDER BY Ano

--3

SELECT * FROM Filmes
WHERE Nome = 'De Volta para o Futuro'

--4
SELECT * FROM Filmes
WHERE Ano = 1997

--5
SELECT * FROM Filmes
WHERE Ano LIKE '200%'

--6
SELECT * FROM Filmes
WHERE Duracao > 100 AND Duracao < 150 
ORDER BY Duracao ASC

--7 
SELECT
Ano,
COUNT(*)AS Quantidade 
 FROM Filmes
Group BY Ano
Order BY Ano

Select * from Filmes

--8

Select * From Atores
WHERE Genero = 'M'

--9 
Select 
Id, 
PrimeiroNome,
UltimoNome,
Genero
FROM Atores
WHERE Genero = 'F'
Order BY PrimeiroNome, UltimoNome

--10 
Select 
Filmes.Id,
Filmes.Nome,
Generos.Genero
FROM
Filmes,Generos


--11 

Filmes.Nome,
Generos.Genero
FROM Filmes, Generos
WHERE Genero = 'MistÃ©rio'

select * From Generos

--12 
SELECT 
Filmes.Nome,
Atores.PrimeiroNome,
Atores.UltimoNome,
ElencoFilme.Papel
FROM Filmes, ElencoFilme,Atores
